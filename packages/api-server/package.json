{
  "name": "@scramjet/api-server",
  "version": "0.35.3",
  "description": "This package is part of Scramjet Transform Hub. The package provides the domain model for STH and the CLI.",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "../../scripts/build-all.js --config-name=tsconfig.build.json --copy-dist",
    "build:docs": "typedoc",
    "clean": "rm -rf ./dist .bic_cache",
    "test": "npm run test:ava",
    "test:ava": "ava -T 50000"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "0http": "^3.4.1",
    "@scramjet/model": "^0.36.1",
    "@scramjet/obj-logger": "^0.36.1",
    "@scramjet/symbols": "^0.36.1",
    "@scramjet/utility": "^0.36.1",
    "http-status-codes": "^2.2.0",
    "scramjet": "^4.36.9"
  },
  "devDependencies": {
    "@scramjet/types": "^0.36.1",
    "@types/node": "15.12.5",
    "@types/sinon": "^10.0.13",
    "@types/trouter": "^3.1.1",
    "ava": "^3.15.0",
    "sinon": "^14.0.1",
    "trouter": "^3.2.0",
    "ts-node": "^10.9.1",
    "typedoc": "0.23.17",
    "typedoc-plugin-markdown": "3.13.6",
    "typescript": "~4.7.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
