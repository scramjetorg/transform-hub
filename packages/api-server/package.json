{
  "name": "@scramjet/api-server",
  "version": "0.10.0",
  "description": "This package is part of Scramjet Transform Hub. The package provides the domain model for STH and the CLI.",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "clean": "rm -rf ./dist .bic_cache",
    "test": "npm run test:ava",
    "test:ava": "ava",
    "prepack": "node ../../scripts/publish.js",
    "postbuild": "yarn prepack"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "0http": "^3.1.0",
    "@scramjet/logger": "^0.10.0",
    "@scramjet/model": "^0.10.0",
    "@scramjet/symbols": "^0.10.0",
    "http-status-codes": "^2.1.4",
    "scramjet": "^4.35.20"
  },
  "devDependencies": {
    "@scramjet/types": "^0.10.0",
    "@types/node": "15.12.5",
    "@types/trouter": "^3.1.0",
    "ava": "^3.15.0",
    "sinon": "^11.1.1",
    "trouter": "^3.2.0",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.4",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.3.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
