{
  "name": "@scramjet/supervisor",
  "version": "0.10.7-0",
  "description": "This package is part of Scramjet Transform Hub. The package executes the remote runners and provides communication with them throgh abstraction layer provided by adapters.",
  "main": "./src/bin/supervisor.ts",
  "scripts": {
    "test": "nyc ava",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "start": "npm run build && node ../../dist/supervisor/bin/supervisor.js ",
    "stop": "./stop-superviosor.sh",
    "prepack": "node ../../scripts/publish.js",
    "clean": "rm -rf ./dist .bic_cache",
    "postbuild": "yarn prepack"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@scramjet/types": "^0.10.7-0",
    "@types/dockerode": "^3.2.2",
    "@types/node": "15.12.5",
    "@types/proxyquire": "^1.3.28",
    "@types/shell-escape": "^0.2.0",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "shell-escape": "^0.2.0",
    "sinon": "^11.1.1",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.4",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.3.4"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@scramjet/adapters": "^0.10.7-0",
    "@scramjet/logger": "^0.10.7-0",
    "@scramjet/model": "^0.10.7-0",
    "@scramjet/sth-config": "^0.10.7-0",
    "@scramjet/symbols": "^0.10.7-0",
    "@scramjet/utility": "^0.10.7-0",
    "bpmux": "^8.1.3",
    "dockerode": "^3.3.0",
    "scramjet": "^4.35.20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
