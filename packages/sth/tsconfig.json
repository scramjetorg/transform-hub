{
  "compilerOptions": {
    "outDir": "./dist"
  },
  "extends": "../../tsconfig.base.json",
  "include": [
    "./src/**/*.ts",
    "./test/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "entryPoints": [
      "src/bin/index.ts"
    ],
    "out": "../../docs/cli",
    "plugin": "typedoc-plugin-markdown",
    "readme": "README.md",
    "excludePrivate": "true"
  }
}
