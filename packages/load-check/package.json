{
  "name": "@scramjet/load-check",
  "version": "0.18.1",
  "description": "This package is part of Scramjet Transform Hub. The package provides health info for STH.",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "clean": "rm -rf ./dist .bic_cache",
    "cloc": "cloc src --fullpath --include-lang TypeScript --not-match-d \"(node_modules|test|dist|bdd)\" --by-percent cm",
    "test": "nyc ava",
    "prepack": "node ../../scripts/publish.js",
    "postbuild": "yarn prepack"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/model": "^0.18.1",
    "@scramjet/obj-logger": "^0.18.1",
    "@scramjet/sth-config": "^0.18.1",
    "@scramjet/symbols": "^0.18.1",
    "@scramjet/utility": "^0.18.1",
    "scramjet": "^4.36.6",
    "systeminformation": "^5.11.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@scramjet/types": "^0.18.1",
    "@types/node": "15.12.5",
    "@types/uuid": "^8.3.4",
    "ava": "^3.15.0",
    "ts-node": "^10.5.0",
    "typedoc": "0.22.11",
    "typedoc-plugin-markdown": "3.11.12",
    "typescript": "^4.5.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
