{
  "name": "@scramjet/manager-api-client",
  "version": "0.19.0",
  "description": "Scramjet Manager API Client",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./dist .bic_cache",
    "watch": "tsc -b --watch",
    "test": "npm run test:ava",
    "test:ava": "ava",
    "prepack": "node ../../scripts/publish.js",
    "postbuild": "yarn prepack"
  },
  "dependencies": {
    "@scramjet/api-client": "^0.19.0",
    "@scramjet/client-utils": "^0.19.0"
  },
  "devDependencies": {
    "@scramjet/types": "^0.19.0",
    "@types/node": "15.12.5",
    "ava": "^3.15.0",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.5.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/scramjet-cpm-dev.git"
  }
}
