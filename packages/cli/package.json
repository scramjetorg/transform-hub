{
  "name": "@scramjet/cli",
  "version": "0.10.0-pre.10",
  "description": "Package template",
  "main": "./src/index.ts",
  "bin": {
    "si": "./src/bin/index.ts"
  },
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./dist .bic_cache",
    "watch": "tsc -b --watch",
    "test": "echo no tests yet -- # npm run test:ava",
    "test:ava": "ava",
    "preinstall": "tar --version 2>&1 >/dev/null",
    "prepack": "node ../../scripts/publish.js",
    "postbuild": "yarn prepack"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/api-client": "0.10.0-pre.10",
    "commander": "^8.0.0",
    "minimatch": "^3.0.4",
    "scramjet": "^4.35.20",
    "tar": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "15.12.5",
    "ava": "^3.15.0",
    "ts-node": "^10.0.0",
    "typedoc": "0.21.2",
    "typedoc-plugin-markdown": "3.10.2",
    "typescript": "^4.3.4",
    "@types/minimatch": "^3.0.4",
    "@types/tar": "^4.0.4"
    },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
