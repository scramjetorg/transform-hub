{
  "ts-node": {
    "files": true
  },
  "compilerOptions": {
    "outDir": "./dist"
  },
  "extends": "../../tsconfig.base.json",
  "include": [
    "./src/**/*.ts",
    "./test/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts"
    ],
    "out": "../../docs/cli",
    "plugin": "typedoc-plugin-markdown",
    "excludePrivate": "true",
    "gitRevision": "HEAD",
    "sort": "alphabetical"
  }
}