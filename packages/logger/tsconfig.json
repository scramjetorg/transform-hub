{
    "compilerOptions": {
        "outDir": "./dist"
    },
    "extends": "../../tsconfig.base.json",
    "include": [
      "./src",
      "./test"
    ],
    "exclude": [
        "node_modules"
    ],
    "typedocOptions": {
      "entryPoints": ["src/index.ts"],
      "out": "../../docs/logger",
      "plugin": "typedoc-plugin-markdown",
      "readme": "none",
      "excludePrivate": "true"
  }
}
